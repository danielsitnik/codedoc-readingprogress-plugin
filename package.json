  "bugs": {
    "url": "https://github.com/danielsitnik/codedoc-readingprogress-plugin/issues"
  },
  "homepage": "https://github.com/danielsitnik/codedoc-readingprogress-plugin#readme",
  "peerDependencies": {
    "@codedoc/core": ">=0.2.5"
  },
  "devDependencies": {
    "@codedoc/core": "^0.2.24",
    "typescript": "^3.9.10"
  }
