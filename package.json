{
  "name": "codedoc-readingprogress-plugin",
  "version": "1.0.0",
  "description": "A reading progress bar plugin for Codedoc.",
  "main": "dist/es5/index.js",
  "module": "dist/es6/index.js",
  "types": "dist/es6/index.d.ts",
  "sdhTransport": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
